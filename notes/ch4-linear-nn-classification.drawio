        <mxCell id="mNR409C78DvK4epMHyib-27" value="$$\mathbf{1}(f(X) \neq Y)$$" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" parent="1" vertex="1">
