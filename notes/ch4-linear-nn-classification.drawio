        <mxCell id="exyy5H1UtoLq-zvEwzxl-19" value="Also called the cross-entropy loss" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=1;strokeColor=default;" parent="1" vertex="1">
